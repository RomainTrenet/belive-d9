msgid "\"[registration:nid:entity:title]\", registration pending | Band Booking"
msgstr "\"[registration:nid:entity:title]\", inscription en attente | Band Booking"

msgid "<p>Hello [registration:registration_user_id:entity:display-name],</p><p>Your registration for the \"[registration:nid:entity:title]\" event is pending, please remember to indicate your availability<a href=\"[registration:url]/edit\">by clicking here</a>.</p><p>Thank you in advance, [registration:uid:entity:display-name].</p>"
msgstr "<p>Bonjour [registration:registration_user_id:entity:display-name],</p><p>Votre inscription à \"[registration:nid:entity:title]\" est en attente, pensez à donner votre disponibilité <a href=\"[registration:url]/edit\">en cliquant ici</a>.</p><p>Merci d'avance, [registration:uid:entity:display-name].</p>"

msgid "[registration:uid:entity:display-name] has booked you on the event [registration:nid:entity:title] | Band booking"
msgstr "[registration:uid:entity:display-name] vous a inscrit à l'évènement [registration:nid:entity:title] | Band booking"

msgid "<p>Hello [registration:registration_user_id:entity:display-name],</p><p>You have been added to the performance \"[registration:nid:entity:title]\".&nbsp;<br>Please inform me of your presence <a href=\"[registration:url]\">by clicking here</a>.</p><p>Thank you in advance,&nbsp;<br>[registration:uid:entity:display-name].</p>"
msgstr "<p>Bonjour [registration:registration_user_id:entity:display-name],</p><p>Vous avez été ajouté(e) à la prestation \"[registration:nid:entity:title]\".&nbsp;<br>Veuillez me prévenir de votre présence <a href=\"[registration:url]\">en cliquant ici</a>.</p><p>Merci d'avance,&nbsp;<br>[registration:uid:entity:display-name].</p>"

msgid "[registration:uid:entity:display-name] removed you from the event [registration:nid:entity:title] | Band booking"
msgstr "[registration:uid:entity:display-name] vous a retiré de l'évènement [registration:nid:entity:title] | Band booking"

msgid "<p>Hello [registration:registration_user_id:entity:display-name],</p><p>You have been removed from the service \"[registration:nid:entity:title]\".</p><p>Sincerely,&nbsp;<br>[registration:uid:entity:display-name].</p>"
msgstr "<p>Bonjour [registration:registration_user_id:entity:display-name],</p><p>Vous avez été retiré(e) de la prestation \"[registration:nid:entity:title]\".</p><p>Cordialement,&nbsp;<br>[registration:uid:entity:display-name].</p>"

